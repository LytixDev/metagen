--- SOURCE ---
func main(): s32
begin   
    var i: s32
    i := 42

    if i > 25 then
    begin
        if i > 50 then
            print 10
        else 
        begin
            if i > 30 then
                print 1
            else
                print 100
            print 2
        end
        print 3
    end
end
--- TREE ---
