   STMT_BLOCK vars=i: s32
    AST_LIST
     STMT_ASSIGNMENT
      EXPR_LITERAL i
      EXPR_LITERAL 42
     STMT_IF
      EXPR_BINARY
       op: GREATER
       EXPR_LITERAL i
       EXPR_LITERAL 25
      STMT_BLOCK vars=
       AST_LIST
        STMT_IF
         EXPR_BINARY
          op: GREATER
          EXPR_LITERAL i
          EXPR_LITERAL 50
         STMT_PRINT
          EXPR_LITERAL 10
         STMT_BLOCK vars=
          AST_LIST
           STMT_IF
            EXPR_BINARY
             op: GREATER
             EXPR_LITERAL i
             EXPR_LITERAL 30
            STMT_PRINT
             EXPR_LITERAL 1
            STMT_PRINT
             EXPR_LITERAL 100
           STMT_PRINT
            EXPR_LITERAL 2
