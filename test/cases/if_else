--- SOURCE ---
func main(): s32
begin   
    var i: s32
    i := 42

    if i = 42 then
        print 1
    else
        print 0

    if i = 0 then
        print 1
    else if i = 42 then
        print 2
    else
        print 3

    if i = 0 then
        print 1
    else if i = 1 then
        print 2
    else
        print 3
end
--- OUTPUT ---
1
2
3
