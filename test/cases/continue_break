--- SOURCE ---
func main(): s32
begin   
    var i: s32
    i := 0
    while i < 100 do
    begin
        i := i + 1
        if i > 50 then
            break
        if i > 10 then
            continue
        print i
    end
    print i
end
--- TREE ---
AST_ROOT
 AST_LIST
 AST_LIST
  AST_FUNC name=main parameters=
   STMT_BLOCK vars=i: s32
    AST_LIST
     STMT_ASSIGNMENT
      EXPR_LITERAL i
      EXPR_LITERAL 0
     STMT_WHILE
      EXPR_BINARY
       op: LESS
       EXPR_LITERAL i
       EXPR_LITERAL 100
      STMT_BLOCK vars=
       AST_LIST
        STMT_ASSIGNMENT
         EXPR_LITERAL i
         EXPR_BINARY
          op: PLUS
          EXPR_LITERAL i
          EXPR_LITERAL 1
        STMT_IF
         EXPR_BINARY
          op: GREATER
          EXPR_LITERAL i
          EXPR_LITERAL 50
         STMT_BREAK
        STMT_IF
         EXPR_BINARY
          op: GREATER
          EXPR_LITERAL i
          EXPR_LITERAL 10
         STMT_CONTINUE
        STMT_PRINT
         EXPR_LITERAL i
     STMT_PRINT
      EXPR_LITERAL i
